<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CannotChangeSentQuote</fullName>
    <active>false</active>
    <errorConditionFormula>ISPICKVAL(PRIORVALUE(Status),&quot;Sent (Customer)&quot;)</errorConditionFormula>
    <errorMessage>You cannot modify Quote sent to Customer</errorMessage>
</ValidationRule>
